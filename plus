package BaekJoon;

import java.util.Scanner;

public class boy {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);
        long a = scanner.nextLong();
        long b = scanner.nextLong();
        long c = scanner.nextLong();

        long sum = a + b + c;

        System.out.println(sum);
    }
}
