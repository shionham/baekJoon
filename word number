package baekjoon;

import java.util.Scanner;

public class __02__ {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String input = sc.nextLine();

        int spacecount = 0;

        for (int i = 0; i < input.length(); i++) {
            if (input.charAt(i) == ' ') {
                spacecount++;
            }
        }
        System.out.println(spacecount + 1);
    }
}
