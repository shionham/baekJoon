package baekjoon;

import java.util.Scanner;

public class __02__ {
    public static void main(String[] args) {

        int[] standard = {1, 1, 2,2,2,8};
        int[] input = new int[6];

        Scanner sc = new Scanner(System.in);

        for (int i = 0; i < 6; i++) {
            input[i] = sc.nextInt();
        }
        for (int i = 0; i < 6; i++) {
            System.out.print((standard[i] - input[i]) + " ");

        }
    }
}
